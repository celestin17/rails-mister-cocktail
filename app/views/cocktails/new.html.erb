<div class="container">
  <div class="text-center" >
    <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.button :submit %>
    <% end %>
  </div>

  <div class="text-center">
    <%= link_to "home", cocktails_path %>
  </div>
</div>
